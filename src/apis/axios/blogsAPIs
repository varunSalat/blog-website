import axios from "axios";

const baseURL = "http://localhost:8800/";

const listingAPI = axios.create({
  baseURL,
  headers: {
    Accept: "application/json",
  },
});

const apiWithAuth = axios.create({
  baseURL,
  headers: {
    Accept: "application/json",
  },
  withCredentials: true,
});

export { listingAPI, apiWithAuth };
